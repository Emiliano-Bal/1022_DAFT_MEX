      "source": [
        "# Um jeito mais bonito de achatar listas\n",
        "list_ex = [4, [5, [2, [1]]]]\n",
        "chata = []\n",
        "\n",
        "while len(list_ex) > 0:\n",
        "    elemento = list_ex.pop()\n",
        "    if type(elemento) == list:\n",
        "        list_ex.extend(elemento)\n",
        "    else:\n",
        "        chata.append(elemento)\n",
        "chata[::-1]"
