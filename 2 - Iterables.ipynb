   "source": [
    "## Iterating over items"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 184,
   "metadata": {},
   "outputs": [],
   "source": [
    "my_dict = {\n",
    "            'Chickpeas': 10,\n",
    "            'Beans': 8,\n",
